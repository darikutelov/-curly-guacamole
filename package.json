  "license": "ISC",
  "devDependencies": {
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.2.10",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pm2": "^5.2.0",
    "stripe": "^9.14.0"
  }
}
